{
  "name": "ag-mcp",
  "version": "0.1.0",
  "private": true,
  "description": "Connect all your farm software to one chat",
  "keywords": [
    "agriculture",
    "john-deere",
    "mcp",
    "chat",
    "precision-ag"
  ],
  "scripts": {
    "dev": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && next dev -p 3000",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --testPathPattern=api --setupFilesAfterEnv=<rootDir>/jest.setup.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "test:regression": "tsx scripts/run-api-regression-tests.ts",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.12.3",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-table": "^8.21.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/xml2js": "^0.4.14",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.1",
    "form-data": "^4.0.3",
    "jose": "^6.0.11",
    "lucide-react": "^0.294.0",
    "multiparty": "^4.2.3",
    "next": "14.0.4",
    "next-auth": "^4.24.11",
    "openai": "^5.3.0",
    "prisma": "^6.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-markdown": "^10.1.0",
    "react-table": "^7.8.0",
    "recharts": "^3.0.0",
    "tailwind-merge": "^2.1.0",
    "uuid": "^9.0.1",
    "xml2js": "^0.6.2",
    "xmlrpc": "^1.3.2",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/geojson": "^7946.0.13",
    "@types/jest": "^29.5.8",
    "@types/multiparty": "^4.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "@types/xmlrpc": "^1.3.10",
    "autoprefixer": "^10.4.16",
    "eslint": "^8",
    "eslint-config-next": "14.2.16",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.0.11",
    "node-fetch": "^2.7.0",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.20"
  }
}
